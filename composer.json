{
    "name": "maildocker/maildocker",
    "description": "This library allows you to quickly and easily send emails through Maildocker using PHP.",
    "version": "1.0",
    "keywords": ["Maildocker", "email"],
    "require": {
        "php": ">=5.3",
        "maildocker/maildocker": "dev-master"
    },
    "replace": {
        "maildocker/maildocker-php": "*"
    },
    "type": "library",
    "minimum-stability": "stable",
    "repositories": [{
        "type": "vcs",
        "url": "https://github.com/maildocker/maildocker-php"
    }],
    "autoload": {
        "psr-0": {
            "Maildocker": "maildocker/"
        }
    }
}
